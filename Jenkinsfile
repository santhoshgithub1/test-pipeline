        pipeline {
                        agent any
                        stages {
                                stage('Build') {
                                        steps {
                                        sh 'sleep 5; echo "this is build stage"'   
                                        }
                                }
                                stage('Test'){
                                        steps {
                                        sh 'sleep 5; echo "this is test stage"'
                                        }
                                }
                                stage('Deploy'){
                                        steps {
                                        sh 'sleep 5; echo "this is deploy stage"'
                                        }
                                }
                                stage('My-stage'){
                                        steps {
                                        sh 'sleep 5; echo "this is my stage"'
                                        }
                                }
                        }
                }

